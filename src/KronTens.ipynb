   "source": [
    "import numpy as np\n",
    "\n",
    "def KronTens(ymm, I4):\n",
    "\n",
    "    K1 = I4.shape[0]\n",
    "    m = ymm.shape[0]\n",
    "    K2 = I4.shape[1]\n",
    "    \n",
    "    resu = np.zeros((K1, m*K2, K1, m*K2))\n",
    "\n",
    "    for i in range(K1):\n",
    "        resu[i, :, i, :] = np.kron(ymm, I4[i, :, i, :])\n",
    "    \n",
    "    return resu\n"
   ]
  }
